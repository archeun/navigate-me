{
    "manifest_version": 3,
    "name": "My Navigator",
    "description": "Access your recently visited web pages within a website",
    "version": "1.0",
    "action": {
        "default_popup": "index.html"
    },
    "background": {
        "service_worker": "scripts/background.js"
    },
    "content_scripts": [
        {
            "js": ["scripts/contents.js"],
            "matches": ["<all_urls>"]
        }
    ],
    "icons": {
        "16": "images/icon-16.png",
        "32": "images/icon-32.png",
        "48": "images/icon-48.png",
        "128": "images/icon-128.png"
    },
    "permissions": [
        "storage",
        "activeTab"
    ]
}